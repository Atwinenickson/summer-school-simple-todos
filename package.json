{
  "name": "summer-school-simple-todos",
  "version": "1.0.0",
  "description": "Describe summer-school-simple-todos here",
  "private": true,
  "scripts": {
    "build": "nwb build-web-app",
    "clean": "nwb clean-app",
    "start": "nwb serve-web-app --reload && tsc && npm run lint",
    "lint": "tslint -c tslint.json '**/.ts' '**/.tsx' --exclude '**/node_modules/**'",
    "test": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server"
  },
  "rules": {
    "linebreak-style": 0,
    "global-require": 0,
    "eslint linebreak-style": [
      0,
      "error",
      "windows"
    ]
  },
  "dependencies": {
    "bootstrap": "^3.3.7"
  },
  "devDependencies": {
    "nwb": "0.21.x",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
